{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "title": "jobs",
      "path": "[[workspace]]/content/jobs"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects"
    }
  ],
  "frontMatter.dashboard.mediaSnippet": [
    "{{< caption \"{mediaUrl}\" \"Description\" >}}"
  ],
  "frontMatter.custom.scripts": [
    {
      "title": "Generate social image",
      "script": "./scripts/social-img.js",
      "nodeBin": "~/.local/nvm/versions/node/v14.17.6/bin/node"
    }
  ],
  "frontMatter.panel.freeform": true,
  "frontMatter.taxonomy.frontMatterType": "YAML",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.taxonomy.alignFilename": false,
  "frontMatter.taxonomy.modifiedField": "lastmod",
  "frontMatter.templates.folder": "content/templates",
  "frontMatter.taxonomy.categories": [
    "Productivity",
    "Programming"
  ],
  "frontMatter.taxonomy.tags": [
    "javascript",
    "windows",
    "ios",
    "twitter",
    "yarn"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "defaults",
      "fields": [
        {
          "title": "Template",
          "name": "template",
          "type": "string"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Publish Date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Modified Date",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "Preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Is in Draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Company",
          "name": "company",
          "type": "string"
        },
        {
          "title": "Location",
          "name": "location",
          "type": "string"
        },
        {
          "title": "Range",
          "name": "range",
          "type": "string"
        },
        {
          "title": "Website",
          "name": "url",
          "type": "string"
        },
        {
          "title": "Git URL",
          "name": "github",
          "type": "string"
        },
        {
          "title": "External URL",
          "name": "external",
          "type": "string"
        },
        {
          "title": "iOS URL",
          "name": "ios",
          "type": "string"
        },
        {
          "title": "Android URL",
          "name": "android",
          "type": "string"
        },
        {
          "title": "Featured",
          "name": "featured",
          "type": "boolean"
        },
        {
          "title": "Category",
          "name": "category",
          "type": "string"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ]
}