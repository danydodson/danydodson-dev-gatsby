{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.templates.folder": "content/templates",
  "frontMatter.content.pageFolders": [
    {
      "title": "jobs",
      "path": "[[workspace]]/content/jobs"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects"
    }
  ],
  "frontMatter.taxonomy.slugPrefix": "/",
  "frontMatter.taxonomy.slugSuffix": "/",
  "frontMatter.taxonomy.alignFilename": false,
  "frontMatter.templates.prefix": "yyyy-MM-dd",
  "frontMatter.taxonomy.dateField": "date",
  "frontMatter.taxonomy.dateFormat": "yyyy-MM-dd",
  "frontMatter.taxonomy.modifiedField": "lastmod",
  "frontMatter.taxonomy.categories": [
    "Productivity",
    "Programming"
  ],
  "frontMatter.taxonomy.commaSeparatedFields": [
    "tags"
  ],
  "frontMatter.taxonomy.tags": [
    "javascript",
    "windows",
    "ios",
    "twitter",
    "yarn"
  ],
  "frontMatter.taxonomy.noPropertyValueQuotes": [
    "date",
    "draft",
    "lastmod",
    "featured"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "post",
      "fields": [
        {
          "title": "Template",
          "name": "template",
          "type": "string"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Published",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Modified",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "Cover",
