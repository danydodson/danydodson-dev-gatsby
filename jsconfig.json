{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "jsx": "preserve",
    "strictFunctionTypes": true,
    "sourceMap": true
  },
  "include": [
    "gatsby-node.js",
    "gatsby-config.js",
    "gatsby/**/*.js",
    "src/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}