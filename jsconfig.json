{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "jsx": "react-jsx",
    "sourceMap": true
  },
  "include": [
    "gatsby/**/*.js",
    "content/meta/config.js",
    "src/**/*.js",
    "gatsby-node.js",
    "gatsby-config.js"
  ],
  "exclude": [
    "**/.github/*",
    "**/.cache/*",
    "**/node_modules/*",
    "**/public/*"
  ]
}